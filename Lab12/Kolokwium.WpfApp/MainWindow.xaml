<Window x:Class="Kolokwium.WpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kolokwium.WpfApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <DataGrid Name="ParkDataGrid" DockPanel.Dock="Bottom" SelectionMode="Single" AutoGenerateColumns="False" Margin="0,135,0,0"/>
        <Button Name="Add_Edit_Park_Button" Content="Add or Edit Park" HorizontalAlignment="Left" Margin="36,36,0,0" VerticalAlignment="Top" Height="66" Width="142" Click="Add_Edit_Park_Button_Click"/>
        <Button Name="Delet_Park_Button" Content="Delete Park" HorizontalAlignment="Left" Margin="215,36,0,0" VerticalAlignment="Top" Height="66" Width="142" Click="Delet_Park_Button_Click"/>
        <Button Name="Add_Edit_Tree_Button" Content="Add or Edit Tree" HorizontalAlignment="Left" Margin="401,36,0,0" VerticalAlignment="Top" Height="66" Width="142" Click="Add_Edit_Tree_Button_Click"/>
    </Grid>
</Window>
