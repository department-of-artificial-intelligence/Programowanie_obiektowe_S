<Window x:Class="Lab8.WpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab8.WpfApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="10,10,0,0">
        <Button x:Name="add_st" Content="Dodaj studenta" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="45" Width="190" Click="add_st_Click"/>
        <Button x:Name="del_st" Content="Usuń studenta" HorizontalAlignment="Left" Margin="205,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.076,-0.148" Height="45" Width="190"/>
        <DataGrid x:Name="DataGridStudents" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,60,395,10"/>

    </Grid>
</Window>
