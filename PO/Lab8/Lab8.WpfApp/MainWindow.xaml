        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab8.WpfApp"
        mc:Ignorable="d"
        Title="Students" Height="450" Width="600">
    <Grid>
        <Button Content="Add Student" Margin="10,10,406,357" Click="MainButtonAddStudent_Click"/>
        <Button Content="Delete Student" Margin="208,10,208,357" Click="MainButtonRemoveStudent_Click"/>
        <DataGrid x:Name="DataGridStudents" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,97,10,10"/>
    </Grid>
</Window>
