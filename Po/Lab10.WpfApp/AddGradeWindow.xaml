<Window x:Class="Lab10.WpfApp.AddGradeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab10.WpfApp"
        mc:Ignorable="d"
        Title="AddGradeWindow" Height="450" Width="437">
    <Grid>
        <ComboBox x:Name="ComboBoxGrade" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Width="389" RenderTransformOrigin="0.507,1.063" Height="40"/>
        <TextBox x:Name="SubjectTextBox" HorizontalAlignment="Left" Margin="195,121,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="204" Height="33"/>
        <Label Content="Temat" HorizontalAlignment="Left" Margin="16,121,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Width="170" Height="31">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.937"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Button x:Name="AddGrade" Content="Dodaj" HorizontalAlignment="Left" Margin="10,237,0,0" VerticalAlignment="Top" Width="389" Height="39" Click="AddGrade_Click"/>

    </Grid>
</Window>
