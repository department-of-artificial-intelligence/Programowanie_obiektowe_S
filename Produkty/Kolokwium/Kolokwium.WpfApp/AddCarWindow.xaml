<Window x:Class="Kolokwium.WpfApp.AddCarWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kolokwium.WpfApp"
        mc:Ignorable="d"
        Title="AddCarWindow" Height="450" Width="450">
    <Grid>
        <TextBox x:Name="TextBoxModel" Text="{Binding Path = Model, Mode=TwoWay}" HorizontalAlignment="Left" Height="23"
Margin="102,47,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.495,0.431"/>
        <Label Content="Model:" HorizontalAlignment="Left" Margin="30,44,0,0" VerticalAlignment="Top"/>
        <Button x:Name="ButtonSaveStudent" Content="Save" HorizontalAlignment="Left" Margin="27,195,0,0" VerticalAlignment="Top"
Width="195" Click="ButtonSaveCar_Click" Height="34"/>
        <DatePicker x:Name="DatePickerDateOfParking" SelectedDate="{Binding Path = DateOfBirth, Mode=TwoWay}" HorizontalAlignment="Left"
Margin="102,159,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="Date of park:" HorizontalAlignment="Left" Margin="20,156,0,0" VerticalAlignment="Top" Height="30"
RenderTransformOrigin="0.456,0.515"/>
    </Grid>
</Window>
